# Common aliases for bash/zsh
alias ll='ls -la'
alias la='ls -A'
alias l='ls -CF'
alias ..='cd ..'
alias ...='cd ../..'

# Git aliases
alias gs='git status'
alias gd='git diff'
alias gc='git commit'
alias gp='git push'
alias gl='git log --oneline --graph'
alias gb='git branch'
alias gco='git checkout'

# Development aliases
alias serve='python3 -m http.server'

# System aliases
alias df='df -h'
alias du='du -h'
alias free='free -h'
